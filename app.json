{
  "expo": {
    "name": "WeatherCast",
    "slug": "weather-cast",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/weather-cast-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "sdkVersion": "42.0.0",
      "package": "com.sven.dorner",
      "permissions": ["LOCATION"],
      "icon": "./assets/weather-cast-icon.png"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
